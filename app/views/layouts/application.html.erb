<!DOCTYPE html>
<html>
  <head>
    <title>.:: Auth Challenge ::.</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>

  <body>
    <main>
      <div class="valign-wrapper vertical-align-wrapper">
        <div class="valign full-width">

          <div class="container">
            <div class="row">

              <div class="col s12">
                  <div class="row">
                    <%= render partial: 'layouts/messages' %>
                  </div>
                  <div class="row">
                    <%= yield %>
                  </div>
              </div>

            </div>
          </div>

        </div>
      </div>
    </main>
  </body>
</html>
